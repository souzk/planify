{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "skipLibCheck": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmitOnError": true,
        "baseUrl": ".",
        "paths": {
            "@me/*": [
                "source/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}